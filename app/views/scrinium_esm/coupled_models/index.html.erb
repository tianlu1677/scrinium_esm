<% content_for :title, t('models.coupled') %>
<% add_breadcrumb t('models.coupled'), coupled_models_path, on_nav_bar: true %>

<% content_for :main_block do %>
  <%=
    render partial: 'scrinium_esm/commons/model_list',
    locals: { models: ScriniumEsm::CoupledModel.all }
  %>
  <% if can? :create, ScriniumEsm::CoupledModel.all %>
    <%= link_to t('action.add'), new_coupled_model_path, class: 'button float-right' %>
  <% end %>
<% end %>
