<% content_for :title, t('models.atm') %>
<% add_breadcrumb t('models.sea_ice'), sea_ice_models_path, on_nav_bar: true %>

<% content_for :main_block do %>
  <%=
    render 'scrinium_esm/commons/model_list',
    models: ScriniumEsm::SeaIceModel.all
  %>
  <% if policy(ScriniumEsm::SeaIceModel).create? %>
    <%= link_to t('action.add'), new_sea_ice_model_path, class: 'button float-right' %>
  <% end %>
<% end %>
