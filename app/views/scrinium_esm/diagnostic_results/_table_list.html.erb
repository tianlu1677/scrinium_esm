<table class='table table-striped'>
  <thead>
    <tr>
      <th width="100"><%= t('activerecord.attributes.diagnostic_result.metric_id') %></th>
      <th width="100"><%= t('activerecord.attributes.diagnostic_result.contact_id') %></th>
      <th width="200"><%= t('activerecord.attributes.diagnostic_result.created_at') %></th>
    </tr>
  </thead>
  <tbody>
    <% diagnostic_results.each do |result| %>
      <tr>
        <td>
          <%= link_to result.metric.short_name, result %>
        </td>
        <td>
          <%= link_to result.contact.name, result.contact %>
        </td>
        <td><%= result.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
