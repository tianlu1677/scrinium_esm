<% content_for :title, @diagnostic_result.metric.short_name %>
<%
  add_breadcrumb @diagnostic_result.metric.short_name,
  scrinium_esm.diagnostic_result_path(@diagnostic_result)
%>

<% content_for :left_side do %>
  <%= render 'profile_left' %>
<% end %>

<% content_for :main_block do %>
  <%=
    render partial: 'resources/list',
    locals: {
      app: scrinium_esm,
      resources: @diagnostic_result.resources
    }
  %>
<% end %>
