<% content_for :title, t('models.lnd') %>
<% add_breadcrumb t('models.lnd'), lnd_models_path, on_nav_bar: true %>

<% content_for :main_block do %>
  <%=
    render partial: 'scrinium_esm/commons/model_list',
    locals: { models: ScriniumEsm::LndModel.all }
  %>
  <% if can? :create, ScriniumEsm::LndModel %>
    <%= link_to t('action.add'), new_lnd_model_path, class: 'button float-right' %>
  <% end %>
<% end %>
