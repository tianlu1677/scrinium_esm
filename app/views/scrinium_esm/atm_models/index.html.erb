<% content_for :title, t('models.atm') %>
<% add_breadcrumb t('models.atm'), atm_models_path, on_nav_bar: true %>

<% content_for :main_block do %>
  <%=
    render partial: 'scrinium_esm/commons/model_list',
    locals: { models: ScriniumEsm::AtmModel.all }
  %>
  <% if is_super_admin?(current_user) %>
    <%= link_to t('action.add'), new_atm_model_path, class: 'button right' %>
  <% end %>
<% end %>
